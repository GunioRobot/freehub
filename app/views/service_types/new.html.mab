h1 'New service_type'

error_messages_for :service_type

form_for @service_type do |f|
  labeled_input 'Name', :for => :name do
    f.text_field :name
  end
  labeled_input 'Description', :for => :description do
    f.text_field :description
  end
  p do
    f.submit"Create"
  end
end

link_to 'Back', service_types_path
