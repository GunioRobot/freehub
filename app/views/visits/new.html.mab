h1 'New visit'

error_messages_for :visit

form_for @visit do |f|
  labeled_input'Datetime', :for => :datetime do
    f.datetime_select :datetime
  end
  labeled_input'Volunteer', :for => :volunteer do
    f.check_box :volunteer
  end
  labeled_input'Note', :for => :note do
    f.text_area :note
  end
  p do
    f.submit"Create"
  end
end

link_to 'Back', visits_path
